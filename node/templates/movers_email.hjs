<!doctype html>
<html lang="en">

<head>

</head>

<body>
    <h1>{{description}}</h1>
    <table style="border: 1px solid black; border-collapse: collapse;">
        <tr>
            <th>Name</th>
            <th>Previous</th>
            <th>Current</th>
        </tr>
        {{#each movers}}
        <tr>
            <td style="border: 1px solid black; padding: 0 1em;">{{name}}</td>
            <td style="border: 1px solid black; padding: 0 1em;">{{previous}}</td>
            <td style="border: 1px solid black; padding: 0 1em;">{{current}}</td>
        </tr>
        {{/each}}

    </table>

</body>

</html>